{
  "name": "resource-monitor",
  "displayName": "Resource Monitor",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.69.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:resource-monitor.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "resource-monitor.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "update": "npm exec --yes npm-check-updates@latest -- --upgrade --target=minor && npm update",
    "format": "npm exec prettier -- --write . --ignore-path .gitignore",
    "check": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/vscode": "^1.69.0",
    "@types/node": "16.x",
    "typescript": "^4.7.4"
  }
}
